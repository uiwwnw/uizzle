{
  "name": "uizzle",
  "version": "1.0.0",
  "description": "",
  "main": "src/App.js",
  "scripts": {
    "start": "npm run clean && npm run copy:dev && npm run test",
    "test": "webpack-dev-server --inline --hot --env --config webpack.dev.js",
    "server": "json-server --watch data/index.json",
    "copy": "copyfiles -f ./config/prod/.env ./",
    "copy:dev": "copyfiles -f ./config/dev/.env ./",
    "clean:cache": "npm cache clear --force",
    "clean:all": "npm run clean:cache || npm run clean:node || npm run clean",
    "clean:node": "rmdir node_modules /s /q",
    "clean": "rimraf dist/*",
    "build": "npm run clean && npm run copy && webpack --env --config webpack.prod.js",
    "build:dev": "npm run clean && npm run copy:dev && webpack --env --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiwwnw/uizzle.git"
  },
  "keywords": [
    "es6",
    "game",
    "javascript",
    "puzzle"
  ],
